{
  "name": "@akiojin/gwt",
  "version": "4.10.0",
  "description": "Interactive Git worktree manager with Coding Agent selection (Claude Code / Codex CLI / Gemini CLI)",
  "main": "dist/index.js",
  "bin": {
    "gwt": "bin/gwt.js"
  },
  "type": "module",
  "packageManager": "bun@latest",
  "scripts": {
    "build": "bun run build:cli && bun run build:web",
    "build:cli": "tsc && bun run build:opentui",
    "build:opentui": "bun scripts/build-opentui.ts",
    "build:server": "tsc",
    "build:client": "vite build src/web/client",
    "build:web": "bun run build:client",
    "dev": "tsc --watch",
    "dev:server": "bun run build:server && bun --watch src/web/server/index.ts",
    "dev:client": "vite src/web/client",
    "dev:web": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "start": "bun ./dist/index.js",
    "start:web": "node ./dist/index.js serve",
    "prepare": "husky",
    "pretest": "bun run build:cli",
    "test": "bun test tests/unit tests/logging tests/quality tests/web src",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "git",
    "worktree",
    "claude",
    "claude code",
    "codex",
    "gemini",
    "gpt",
    "cli",
    "interactive",
    "branch",
    "development"
  ],
  "author": "akiojin",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fastify/static": "^9.0.0",
    "@fastify/websocket": "^11.2.0",
    "@opentui/core": "^0.1.68",
    "@opentui/solid": "^0.1.68",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.66.1",
    "@xyflow/react": "^12.10.0",
    "chalk": "^5.4.1",
    "d3-force": "^3.0.0",
    "execa": "^9.6.0",
    "fastify": "^5.2.1",
    "node-pty": "1.1.0",
    "pino": "^10.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "solid-js": "^1.9.10",
    "string-width": "^8.1.0",
    "systray2": "^2.1.4",
    "tailwindcss": "^4.1.18",
    "ws": "^8.18.0",
    "xterm": "^5.4.0-beta.37",
    "xterm-addon-fit": "^0.9.0-beta.37",
    "yaml": "^2.8.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@happy-dom/global-registrator": "^20.0.11",
    "@playwright/test": "^1.51.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/d3-force": "^3.0.10",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.0.3",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "esbuild": "0.27.2",
    "eslint": "^9.31.0",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.7",
    "lucide-react": "^0.562.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "typescript-language-server": "^5.1.3",
    "vite": "^7.2.4"
  },
  "lint-staged": {
    "**/*.md": "bunx --bun markdownlint-cli -c .markdownlint.json --ignore CHANGELOG.md --ignore node_modules",
    "*.{ts,tsx}": [
      "bunx eslint --max-warnings=0 --no-warn-ignored",
      "bunx prettier --check"
    ]
  },
  "files": [
    "dist/",
    "src/",
    "bin/",
    "README.md"
  ]
}
