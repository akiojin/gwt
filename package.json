{
  "name": "@akiojin/gwt",
  "version": "6.1.0",
  "description": "Interactive Git worktree manager with Coding Agent selection (Claude Code / Codex CLI / Gemini CLI)",
  "bin": {
    "gwt": "bin/gwt.js"
  },
  "type": "module",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "prepare": "bunx husky install",
    "lint:husky": "bash scripts/verify-husky-hooks.sh"
  },
  "keywords": [
    "git",
    "worktree",
    "claude",
    "claude code",
    "codex",
    "gemini",
    "cli",
    "interactive",
    "branch",
    "development",
    "rust"
  ],
  "author": "akiojin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akiojin/gwt.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/",
    "scripts/"
  ],
  "engines": {
    "node": ">=18"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
